{
    "kind": "Template",
    "apiVersion": "v1",
    "metadata": {
        "name": "gwells-postgresql-bc",
        "creationTimestamp": null
    },
    "parameters": [
        {
            "name": "NAME_SUFFIX",
            "displayName": "Name Suffix",
            "description": "A suffix appended to all objects",
            "required": true
        },
        {
            "name": "ENV_NAME",
            "required": true
        }
    ],
    "objects": [
        {
            "kind": "BuildConfig",
            "apiVersion": "v1",
            "metadata": {
                "name": "gwells-postgres-postgis-fdw${NAME_SUFFIX}",
                "creationTimestamp": null,
                "labels": {
                    "build": "gwells-postgres-postgis-fdw"
                },
                "annotations": {
                }
            },
            "spec": {
                "successfulBuildsHistoryLimit": 3,
                "failedBuildsHistoryLimit": 3,
                "triggers": [
                    {
                        "type": "ImageChange",
                        "imageChange": {}
                    },
                    {
                        "type": "ConfigChange"
                    }
                ],
                "runPolicy": "SerialLatestOnly",
                "source": {
                    "contextDir": "docker-images/gwells-postgres-postgis-fdw",
                    "git": {
                        "ref": "master",
                        "uri": "${SOURCE_REPOSITORY_URL}"
                    },
                    "type": "Git"
                },
                "output": {
                    "to": {
                        "kind": "ImageStreamTag",
                        "name": "gwells-postgres-postgis-fdw${NAME_SUFFIX}:${ENV_NAME}"
                    }
                },
                "resources": {},
                "postCommit": {},
                "nodeSelector": null
            }
        },
        {
            "kind": "ImageStream",
            "apiVersion": "v1",
            "metadata": {
                "name": "gwells-postgresql${NAME_SUFFIX}",
                "creationTimestamp": null,
                "labels": {
                    "base-name": "gwells-postgresql"
                },
                "annotations": {}
            },
            "spec": {
                "lookupPolicy": {
                    "local": false
                },
                "tags":[
                    {
                        "name": "${ENV_NAME}",
                        "annotations": null,
                        "from": {
                            "kind": "ImageStreamTag",
                            "name": "postgresql-oracle-fdw:9.5-2"
                        },
                        "importPolicy": {},
                        "referencePolicy": {
                            "type": "Source"
                        }
                    }
                ]
            }
        }
    ]
}
